{
  "expo": {
    "name": "splitKaro",
    "slug": "splitKaro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "splitkaro",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitkaro.app",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app does not access your calendar.",
        "NSUserTrackingUsageDescription": "We do not track users."
      }
    },
    "android": {
      "package": "com.splitkaro.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "updates": {
      "url": "",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": ""
  }
}
